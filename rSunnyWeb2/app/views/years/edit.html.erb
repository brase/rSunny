<h1>Editing year</h1>

<% form_for(@year) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :year %><br />
    <%= f.text_field :year %>
  </p>
  <p>
    <%= f.label :sum_unit0 %><br />
    <%= f.text_field :sum_unit0 %>
  </p>
  <p>
    <%= f.label :sum_unit1 %><br />
    <%= f.text_field :sum_unit1 %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @year %> |
<%= link_to 'Back', years_path %>